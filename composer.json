{
  "name": "medikly/application",
  "description": "Medikly main application",
  "homepage": "https://github.com/ragavagroup/application.git",
  "license": "OSL-3.0",
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "repositories": {
    "composer": {
      "type": "composer",
      "url": "http://packagist.orocrm.com"
    },
    "enterprise": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/crm-enterprise.git"
    },
    "CustomDashboardMenu": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/CustomDashboardMenu.git"
    },
    "Event": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/Event.git"
    },
    "Provider": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/Provider.git"
    },
    "DefaultLegacy": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/DefaultLegacy.git"
    },
    "UI-REEA": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/UI-REEA-.git"
    },
    "Theme-REEA": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/Theme-REEA-.git"
    },
    "Segments": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/Segments.git"
    },
    "Getstarted": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/Getstarted.git"
    },
    "Inheritance": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/Inheritance.git"
    },
    "campaigns": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/campaigns.git"
    },
    "dashboard": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/dashboard.git"
    },
    "proxy": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/proxy.git"
    },
    "user-journey": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/user-journey.git"
    },
    "contactsync": {
      "type": "vcs",
      "url": "https://github.com/ragavagroup/contactsync.git"
    },
    "syno-orocrm-fullcontact": {
      "type": "vcs",
      "url": "https://github.com/synolia/SynoOroCRMFullContactBundle.git"
    }
  },
  "require": {
    "oro/crm": "1.6.*",
    "oro/crm-mail-chimp": "~1.0@dev",
    "oro/crm-zendesk": "~1.0@dev",

    "medikly/customdashboardmenu-bundle": "dev-master",
    "medikly/event-bundle": "dev-master",
    "medikly/provider-bundle": "dev-master",
    "medikly/defaultlegacy-bundle": "dev-master",
    "medikly/ui-bundle": "dev-master",
    "medikly/theme-bundle": "dev-master",
    "medikly/segments": "dev-master",
    "medikly/getstarted": "dev-master",
    "medikly/inheritance": "dev-master",
    "medikly/campaigns": "dev-master",
    "medikly/piwik-dashboard": "dev-master",
    "medikly/piwik-proxy": "dev-master",
    "medikly/user-journey": "dev-master",
    "medikly/contactsync": "dev-master",

    "atwix/oro-mandrill": "dev-master",
    "synolia/syno-orocrm-fullcontact": "dev-master"
  },
  "config": {
    "component-dir": "web/bundles/components"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setPermissions",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::installAssets"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setPermissions",
      "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::installAssets"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
